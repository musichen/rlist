<p> ALL LISTINGS </p>

<% @listings.each do |l| %>
ID: <%= l.id %>
Title: <%= l.title %><br>
Description: <%= l.description %><br>
City: <%= l.city %><br>
State: <%= l.state %><br>
Zipcode: <%= l.zipcode %><br>
Category_id: <%= l.category_id %><br>
Category_name: <%= Category.find(l.category_id).name %><br>
Subcategory_id: <%= l.subcategory_id %><br>
Subcategory_name: <%= Subcategory.find(l.subcategory_id).name %><br>
Created_at: <%= l.created_at %><br>
Updated_at: <%= l.updated_at %><br>
<p> ---------------------------------------------------------- </p><br>

<% end %>


